{
    "name": "laravel-bridge/zf1",
    "license": "MIT",
    "description": "The bridge for Laravel on Zend Framework 1",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework1": "^1.12",
        "laravel-bridge/support": "~0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1",
        "psy/psysh": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Zf1\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
